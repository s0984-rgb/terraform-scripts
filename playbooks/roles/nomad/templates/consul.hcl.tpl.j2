{{ '{{' }} with secret "consul/creds/nomad-{{ nomad_template_type }}-role" {{ '}}' }}
consul {
  address = "127.0.0.1:8501"
  grpc_address = "127.0.0.1:8502"
  auto_advertise = true
  ssl = true
  share_ssl = true
  token = "{{ '{{' }} .Data.token {{ '}}' }}"
  grpc_ca_file = "{{ consul_ca }}"
  ca_file = "{{ consul_ca }}"
  cert_file = "/etc/consul.d/certs/consul.pem"
  key_file = "/etc/consul.d/certs/consul.pem"
}
{{ '{{' }} end {{ '}}' }}
